<h1>Here's the list of all wikis</h1>
<% if policy(Wiki.new).create? %>
  <%= link_to "Create new wiki", new_wiki_path, class: 'btn btn-success' %>
  <% end %>
<% @wikis.each do |w| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
      <%= link_to w.title, w %>
    </h4>
    </div>
  </div>
<% end %>
